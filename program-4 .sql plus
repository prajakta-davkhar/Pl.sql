DECLARE
r number;
a number;
BEGIN
FOR r in 5..9 LOOP
a:=3.14*r*r;
INSERT INTO circle(radius,area) VALUES (r,a);
DBMS_OUTPUT.PUT_LINE('radius=' || r || ', area='||a);
END LOOP;
END;
/


